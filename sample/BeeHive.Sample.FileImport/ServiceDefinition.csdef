<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="BeeHive.Sample.FileImport" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2015-04.2.6">
  <WorkerRole name="BeeHive.Sample.FileImport.Worker" vmsize="ExtraSmall">
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="StorageConnectionString" />
      <Setting name="BusConnectionString" />
      <Setting name="ElasticSearchUrl" />
      <Setting name="SweepRootPath" />
      <Setting name="Microsoft.WindowsAzure.Plugins.Diagnostics.ConnectionString"/>
    </ConfigurationSettings>
  </WorkerRole>
</ServiceDefinition>